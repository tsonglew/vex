{
  "name": "vecx",
  "displayName": "Vex - Vector Database Manager",
  "description": "A comprehensive VS Code extension for managing vector databases like Milvus and ChromaDB. Create, manage, and query vector collections with an intuitive interface.",
  "version": "0.0.1",
  "publisher": "tsonglew",
  "icon": "assets/logo.jpeg",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsonglew/vex.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Data Science",
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "vector",
    "database",
    "milvus",
    "chromadb",
    "embeddings",
    "ai",
    "ml"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vex.refreshTreeView",
        "title": "Refresh",
        "icon": "$(refresh)",
        "category": "Vector Database"
      },
      {
        "command": "vex.addConnection",
        "title": "Add Database Connection",
        "icon": "$(add)",
        "category": "Vector Database"
      },
      {
        "command": "vex.connectToDatabase",
        "title": "Connect",
        "icon": "$(plug)",
        "category": "Vector Database"
      },
      {
        "command": "vex.disconnectFromDatabase",
        "title": "Disconnect",
        "icon": "$(debug-disconnect)",
        "category": "Vector Database"
      },
      {
        "command": "vex.editConnection",
        "title": "Edit Connection",
        "icon": "$(edit)",
        "category": "Vector Database"
      },
      {
        "command": "vex.deleteConnection",
        "title": "Delete Connection",
        "icon": "$(trash)",
        "category": "Vector Database"
      },
      {
        "command": "vex.createCollection",
        "title": "Create Collection",
        "icon": "$(new-folder)",
        "category": "Vector Database"
      },
      {
        "command": "vex.insertVectors",
        "title": "Insert Vectors",
        "icon": "$(add)",
        "category": "Vector Database"
      },
      {
        "command": "vex.searchVectors",
        "title": "Search Vectors",
        "icon": "$(search)",
        "category": "Vector Database"
      },
      {
        "command": "vex.listVectors",
        "title": "List Vectors",
        "icon": "$(list-unordered)",
        "category": "Vector Database"
      },
      {
        "command": "vex.deleteCollection",
        "title": "Delete Collection",
        "icon": "$(trash)",
        "category": "Vector Database"
      },
      {
        "command": "vex.viewVectors",
        "title": "View Vectors",
        "icon": "$(list-unordered)",
        "category": "Vector Database"
      },
      {
        "command": "vex.manageCollection",
        "title": "Manage Collection",
        "icon": "$(settings-gear)",
        "category": "Vector Database"
      },
      {
        "command": "vex.viewVectorDetails",
        "title": "View Vector Details",
        "icon": "$(eye)",
        "category": "Vector Database"
      },
      {
        "command": "vex.createDatabase",
        "title": "Create Database",
        "icon": "$(new-folder)",
        "category": "Vector Database"
      },
      {
        "command": "vex.deleteDatabase",
        "title": "Delete Database",
        "icon": "$(trash)",
        "category": "Vector Database"
      },
      {
        "command": "vex.refreshConnectionStatus",
        "title": "Refresh Connection Status",
        "icon": "$(refresh)",
        "category": "Vector Database"
      },
      {
        "command": "vex.refreshDatabases",
        "title": "Refresh Databases",
        "icon": "$(database)",
        "category": "Vector Database"
      },
      {
        "command": "vex.deleteServer",
        "title": "Delete Server",
        "icon": "$(trash)",
        "category": "Vector Database"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vex.addConnection",
          "when": "true"
        },
        {
          "command": "vex.refreshTreeView",
          "when": "true"
        }
      ],
      "view/title": [
        {
          "command": "vex.addConnection",
          "when": "view == vexVectorDBTree",
          "group": "navigation@1"
        },
        {
          "command": "vex.refreshTreeView",
          "when": "view == vexVectorDBTree",
          "group": "navigation@2"
        },
        {
          "command": "vex.refreshDatabases",
          "when": "view == vexVectorDBTree",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "vex.connectToDatabase",
          "when": "view == vexVectorDBTree && viewItem == serverConnection",
          "group": "connection@1"
        },
        {
          "command": "vex.disconnectFromDatabase",
          "when": "view == vexVectorDBTree && viewItem == serverConnection",
          "group": "connection@2"
        },
        {
          "command": "vex.editConnection",
          "when": "view == vexVectorDBTree && viewItem == databaseConnection",
          "group": "management@1"
        },
        {
          "command": "vex.deleteConnection",
          "when": "view == vexVectorDBTree && viewItem == databaseConnection",
          "group": "management@2"
        },
        {
          "command": "vex.deleteServer",
          "when": "view == vexVectorDBTree && viewItem == serverConnection",
          "group": "management@2"
        },
        {
          "command": "vex.createDatabase",
          "when": "view == vexVectorDBTree && viewItem == serverConnection",
          "group": "databases@1"
        },
        {
          "command": "vex.refreshConnectionStatus",
          "when": "view == vexVectorDBTree && viewItem == serverConnection",
          "group": "connection@3"
        },
        {
          "command": "vex.createCollection",
          "when": "view == vexVectorDBTree && viewItem == database",
          "group": "collections@1"
        },
        {
          "command": "vex.deleteDatabase",
          "when": "view == vexVectorDBTree && viewItem == database",
          "group": "databases@2"
        },
        {
          "command": "vex.listVectors",
          "when": "view == vexVectorDBTree && viewItem == collection",
          "group": "collection@1"
        },
        {
          "command": "vex.insertVectors",
          "when": "view == vexVectorDBTree && viewItem == collection",
          "group": "collection@2"
        },
        {
          "command": "vex.searchVectors",
          "when": "view == vexVectorDBTree && viewItem == collection",
          "group": "collection@3"
        },
        {
          "command": "vex.manageCollection",
          "when": "view == vexVectorDBTree && viewItem == collection",
          "group": "collection@4"
        },
        {
          "command": "vex.deleteCollection",
          "when": "view == vexVectorDBTree && viewItem == collection",
          "group": "collection@5"
        },
        {
          "command": "vex.viewVectors",
          "when": "view == vexVectorDBTree && viewItem == vectorsGroup",
          "group": "vectors@1"
        },
        {
          "command": "vex.viewVectorDetails",
          "when": "view == vexVectorDBTree && viewItem == vector",
          "group": "vector@1"
        }
      ]
    },
    "views": {
      "vexVectorDB": [
        {
          "id": "vexVectorDBTree",
          "name": "Vector Databases",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vexVectorDB",
          "title": "Vector Database",
          "icon": "$(database)"
        }
      ]
    }
  },
  "dependencies": {
    "@zilliz/milvus2-sdk-node": "^2.6.0",
    "axios": "^1.6.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  }
}